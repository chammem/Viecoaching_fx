<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="597.0" prefWidth="1096.0" style="-fx-background-color: #EDEEE1;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AfficheCatController">
    <VBox alignment="CENTER" layoutY="78.0" prefHeight="518.0" prefWidth="811.0" style="-fx-background-color: #EDEEE1;">
        <ScrollPane fitToHeight="true" fitToWidth="true">
            <content>
                <!-- GridPane pour afficher les ressources -->
                <GridPane fx:id="gridPane" hgap="10" prefHeight="354.0" prefWidth="810.0" vgap="10">
                </GridPane>
            </content>
        </ScrollPane>
        <Button mnemonicParsing="false" onAction="#AjoutCat" prefHeight="44.0" prefWidth="262.0" style="-fx-background-color: #BED6B1;" text="Ajouter Catégorie" />
    </VBox>
    <TextField fx:id="tText" layoutX="25.0" layoutY="17.0" prefHeight="30.0" prefWidth="218.0" promptText="Recherche" style="-fx-background-color: #BED6B1;">
        <font>
            <Font name="System Italic" size="14.0" />
        </font></TextField>

    <Label layoutX="323.0" layoutY="14.0" style="-fx-font-size: 24px; -fx-font-weight: bold;" text="List catégorie" textFill="#a5b79b" />
</AnchorPane>
